SELECT OrderDetails.OrderID, OrderDetails.UnitPrice * OrderDetails.Quantity
FROM OrderDetails
WHERE OrderID=4
GROUP BY OrderID;

SELECT OrderID, SUM(UnitPrice) * SUM(Quantity)
FROM OrderDetails
WHERE UnitPrice * Quantity > 1000
GROUP BY OrderID;

